{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist/renderer",
  "devCommand": "npm run dev:web",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

