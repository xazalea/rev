{
  "buildCommand": "npm run build:renderer",
  "outputDirectory": "dist/renderer",
  "devCommand": "npm run dev:renderer",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "OPENROUTER_API_KEY": "@openrouter_api_key"
  }
}

